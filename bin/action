#!/usr/bin/env bash
# Generated by eigenbot-app/generate-go-action; DO NOT EDIT.

set -e

if ! command -v gh &>/dev/null; then
  echo "::error::Expected GitHub CLI \`gh\` in PATH."
  exit 1
fi

echo "Downloading Action \`download-release\`..."
gh release download "" \
  --repo "eigenbot-app/download-release" \
  --pattern "download-release" \
  --dir .

chmod +x "./download-release"
"./download-release" "$@"
